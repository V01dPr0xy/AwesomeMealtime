<Window x:Class="DataPersistance_One.Models.RemoveContactDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataPersistance_One.Models"
        mc:Ignorable="d"
        Title="RemoveContactDialog" Height="120" Width="200">
    <Grid>
        <ComboBox Name="cb_list" HorizontalAlignment="Left" Height="26" Margin="10,18,0,0" VerticalAlignment="Top" Width="150"/>
        <Button Name="btn_remove" Content="Remove" HorizontalAlignment="Left" Height="27" Margin="17,49,0,0" VerticalAlignment="Top" Width="59" Click="btn_remove_Click"/>
        <Button Name="btn_cancel" Content="Cancel" HorizontalAlignment="Left" Height="27" Margin="92,49,0,0" VerticalAlignment="Top" Width="59" Click="btn_cancel_Click"/>

    </Grid>
</Window>
